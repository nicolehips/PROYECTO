    <div class="container">
        <h1 class="text-center pt-5">Actualizar un suscriptor</h1>
        <h3 class="text-center">Actualiza los suscriptores del sistema.</h3>
        <div class="row pt-3">
          <div class="col"></div>
          <div class="col-6 pb-5">
            <div class="card p-5">
              <form method="POST" action="/actualizar">
                <div class="mb-3">
                  <label for="formGroupExampleInput" class="form-label">Nombre:</label>
                  <input type="text" class="form-control" id="formGroupExampleInput" Name="nombre"
                    placeholder="Llene el campo "required>
                </div>
                <div class="mb-3">
                  <label for="formGroupExampleInput2" class="form-label">Correo:</label>
                  <input type="text" class="form-control" id="formGroupExampleInput2" Name="correo"
                    placeholder="Llene el campo"required>
                </div>
                <div class="mb-3">
                  <label for="formGroupExampleInput" class="form-label">Cédula:</label>
                  <input type="text" class="form-control" id="formGroupExampleInput" Name="cedula"
                    placeholder="Llene el campo"required >
                </div>
              
                <div class="mb-3">
                  <label for="formGroupExampleInput2" class="form-label">Sucursal:</label>
                  <input type="text" class="form-control" id="formGroupExampleInput2" Name="sucursal"
                    placeholder="Llene el campo "required>
                </div>
                <div class="mb-3">
                  <label for="formGroupExampleInput2" class="form-label">Tipo de Suscipción:</label>
                  <input type="text" class="form-control" id="formGroupExampleInput2" Name="suscripcion"
                    placeholder="Llene el campo "required>
                </div>
                <div class="mb-3">
                  <label for="formGroupExampleInput2" class="form-label">Costo de la Suscripcion:</label>
                  <input type="text" class="form-control" id="formGroupExampleInput2" Name="costo"
                    placeholder="Llene el campo "required>
                </div>
                <div class="mb-3">
                  <label for="formGroupExampleInput2" class="form-label">Saldo Pendiente:</label>
                  <input type="text" class="form-control" id="formGroupExampleInput2" Name="moroso"
                    placeholder="Llene el campo"required>
                </div>
                <div class="mb-3">
                  <label for="formGroupExampleInput2" class="form-label">Fecha:</label>
                  <input type="date" class="form-control" id="formGroupExampleInput2" Name="fechaDS"
                    placeholder="Llene el campo "required>
                </div>
                <div class="row">
                  <div class="col text-center">
                    <button type="submit" class="btn btn-success  m-3 p5">Actualizar</button>
                  </div>
                </div>
              </form>
            </div>
          </div>
          <div class="col"></div>
        </div>
      </div>


